<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile_bg">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FundingAgencyProfile">


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:padding="10dp"
        android:text="Funding Agency Profile"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.541"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/roundedcorners"/>

    <TextView
        android:id="@+id/fa_profile_type"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:text="Type of Agency"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.176"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/show_details_btn" />

    <TextView
        android:id="@+id/fa_profile_agency_type"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:textColor="@color/white"
        android:text="Government"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_type"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_type"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_type" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:textColor="@color/white"
        android:layout_marginTop="36dp"
        android:text="Name of Funding Agency"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_agency_type"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_agency_type"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_agency_type" />

    <TextView
        android:id="@+id/fa_profile_funding_agency"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:text="AICTE"
        android:textColor="@color/white"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/fa_profile_year"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:textColor="@color/white"
        android:text="1945"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="@+id/textView21"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:text="Name of Founder"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_year"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_year"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_year" />

    <TextView
        android:id="@+id/fa_profile_name_of_founder"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:text="Anil Sahasrabudhe"
        android:layout_marginTop="28dp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView22"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="12dp"
        android:text="Phone Number"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_name_of_founder"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_name_of_founder"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_name_of_founder" />

    <TextView
        android:id="@+id/fa_profile_phone_number"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:text="8022205979"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView24"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="@+id/textView24"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="32dp"
        android:textColor="@color/white"
        android:text="port folio link"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_phone_number"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_phone_number"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_phone_number" />

    <TextView
        android:id="@+id/fa_portfolio_link"
        android:layout_width="167dp"
        android:text="https://www.aicte-india.org/schemes"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView26"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:text="social link"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/fa_portfolio_link"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_portfolio_link"
        app:layout_constraintTop_toBottomOf="@+id/fa_portfolio_link" />

    <TextView
        android:id="@+id/fa_profile_social_link"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="16dp"
        android:text="https://twitter.com/AICTE_INDIA"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView28"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="16dp"
        android:text="state"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_social_link"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_social_link"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_social_link" />

    <TextView
        android:id="@+id/fa_profile_state"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:text="New Delhi"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/textView30"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView30" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="28dp"
        android:text="district"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_state"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_state"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_state" />

    <TextView
        android:id="@+id/fa_profile_district"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="16dp"
        android:textColor="@color/white"
        android:text="New Delhi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView32"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView32"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:textColor="@color/white"
        android:text="Year of Establishment"
        app:layout_constraintEnd_toEndOf="@+id/fa_profile_funding_agency"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="@+id/fa_profile_funding_agency"
        app:layout_constraintTop_toBottomOf="@+id/fa_profile_funding_agency"
        android:layout_marginBottom="10dp"/>

    <Button
        android:id="@+id/show_details_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="172dp"
        android:background="@drawable/roundedcorners"
        android:text="show details"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <ImageView
        android:id="@+id/image_view_fa_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/show_details_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.704"
        tools:srcCompat="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>